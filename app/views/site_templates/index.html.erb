

<div class="container">

  <div class>
    <h1>Welcome to Tweesta <h3>Find photos and tweets with the same tag</h3></h1>
  </div>

  <div class="row text-center col-md-offset-3 col-md-6">

      <form ng-submit="tweestas.searchTag(newTag)">
        <input type="text" ng-model="newTag" autofocus required placeholder="Search by tag" class="form-control text-center">
        <button class="form-control btn btn-primary">Search</button>
      </form>
  </div>

  <div class="row">
    <button ng-click="tweestas.loadMore(newTag)" ng-model="newTag" class="col-md-offset-4 col-md-4 text-center btn btn-info">For More</button>
  </div>

  <div class="container">

    
    <div class="row col-md-4">
        
        <h1 class="text-center">TWITTER TWEETS</h1> 

       <ul ng-repeat="saved in tweestas.saved">
          <blockquote ng-repeat="obj in saved.twitter">
            {{obj.text}}{{$index}}
          </blockquote>
        </ul>

    </div>

    <div class="row col-md-6">

      
        <h1 class="text-center">INSTAGRAM PHOTOS</h1>

        <ul ng-repeat="save in tweestas.saved">
          <ul ng-repeat="obj in save.instagram">
            <div class="thumbnail col-md-6">
              <img src="{{obj.images.low_resolution.url}}" alt="instagram photo" class="img-responsive img-rounded">
              <p class="caption">{{obj.caption.text}}</p>
            </div>
          </ul>
        </ul>
        

    </div>

  </div>



</div>
