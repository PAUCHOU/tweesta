

<div class="container">
  
  <div class>
    <h1>Welcome to Tweesta</h1>
  </div>

  <div class="row text-center col-md-offset-3 col-md-6">
    
      <form ng-submit="tweestas.searchTag(newTag)">
        <input type="text" ng-model="newTag" autofocus required placeholder="Search tags" class="form-control text-center">
        <button class="form-control btn btn-primary">Search</button>
      </form>
  </div>

  <div class="row">
    <button ng-click="tweestas.loadMore(newTag)" ng-model="newTag" class="col-md-offset-4 col-md-4 text-center btn btn-info">For More</button>
  </div>

  <div class="container">
    
    <div class="row col-md-6">
        
        <h1>TWITTER TWEETS</h1> 
      
        <ul ng-repeat="tweet in tweestas.results.twitter">
          <p>{{tweet.text}}</p>
        </ul>

    </div>

    <div class="row col-md-6">
      
        <h1>INSTAGRAM PHOTOS</h1>
<!-- 
        <ul ng-repeat="photo in tweestas.results.instagram">
          <img src="{{photo.images.low_resolution.url}}" alt="instagram photo">
        </ul> -->

        <ul ng-repeat="photo in tweestas.saved[0].instagram">
          <img src="{{photo.images.low_resolution.url}}" alt="instagram photo">
        </ul>
        
    </div>

  </div>



</div>
