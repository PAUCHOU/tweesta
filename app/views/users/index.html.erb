
<h1>Users#index</h1>

<div class="container">

  <div class="row col-md-6">


    <h2>Instagram Photos</h2>

    <% @results.each do |x| %>
      <p>
        <img src="<%= x["images"]["low_resolution"]["url"]%>">
      </p>
    <% end %>

  </div>

  <div class="row col-md-6">

    <h2>Tweets</h2>

    <% @twitter_results.each do |tweets| %>
      <p>
        <%= tweets.text %>
      </p>
    <% end %>

  </div>

  <div class="row">
    <button class="btn btn-info">For More</button>
  </div>

</div>

